{
  "name": "Reboost",
  "version": "1.0.0",
  "description": "Reboost quiz",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "nodemon",
    "start": "node dist/index.js",
    "lint": "./node_modules/.bin/tslint --project ./tsconfig.json -e 'src/test/**/*.*'",
    "lint:fix": "./node_modules/.bin/tslint --project ./tsconfig.json -e 'src/test/**/*.*'  --fix",
    "test": "ENV=test nyc mocha -r ts-node/register -r dotenv/config src/test/**/*.spec.ts --exit",
    "coverage": ". && nyc -r text-summary -r html npm run test"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "ISC",
  "nyc": {
    "cache": false,
    "check-coverage": false,
    "extension": [
      ".ts"
    ],
    "include": [
      "lib/**/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "coverage/**",
      "src/test/**",
      "src/config/environments",
      "lib/interfaces",
      "node_modules/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": false,
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true,
    "instrument": true
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@types/express": "^5.0.0",
    "@types/validator": "^13.12.0",
    "@types/winston": "^2.4.4",
    "ajv": "^6.10.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "core-decorators": "^0.20.0",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.2",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.15",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^2.0.1",
    "Reboost": "file:",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.33.5",
    "strong-params": "^1.0.0",
    "supertest": "^7.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3",
    "uuid": "^10.0.0",
    "winston": "^3.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.11",
    "@types/node": "^12.12.29",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-sinon": "^1.0.24"
  }
}
